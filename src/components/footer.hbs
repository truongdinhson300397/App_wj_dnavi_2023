{{#block "footer-scripts"}}
    <script>
        var assetsPath = '{{path}}';
        var domain = '{{domain}}';
    </script>
    <!--    libraries-->
    <script src="{{path}}js/vendor/jquery-2.2.4.min.js"></script>
    <script src="{{path}}js/vendor/jquery-ui.min.js"></script>
    <script src="{{path}}js/vendor/jquery.easing.min.js"></script>
    <script src="{{path}}js/vendor/moment.js"></script>
    <script src="{{path}}js/vendor/moment-timezone-with-data.min.js"></script>
    <script src="{{path}}js/vendor/lodash.min.js"></script>
    {{#unless isApplican}}
        <script src="{{path}}js/jquery.cookie.js"></script>
        <script src="{{path}}js/shared/function_controller.js"></script>
    {{else}}
        <script>
            var isApplican = true;
        </script>
        <script src="{{path}}js/applican.helpers.js"></script>
    {{/unless}}
    {{#if isApplican}}
    <script>
        globalInfo('contract_term_id', {{contractTermId}});
        globalInfo('contract_term', {{contractTerm}});
    </script>
    {{/if}}
    <script src="{{path}}js/shared/nav_component.js"></script>
    <script src="{{path}}js/swiper.js"></script>
    <script src="{{path}}js/common.js"></script>
    <!-- common functions -->
    <script src="{{path}}js/functions.js"></script>
    <script src="{{path}}js/gumi_helpers.js"></script>
    <script src="{{path}}js/shared/general.js"></script>
    {{#unless isApplican}}
    <script  src="{{path}}js/shared/footer_component.js"></script>
    {{/unless}}
    <script src="{{path}}js/sites/apiSend.js"></script>
    <script src="{{path}}js/beacon_checkin.js"></script>
{{/block}}
